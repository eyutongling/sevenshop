/*通用样式*/
*{
    margin: 0;
    padding: 0;
    font-family: "微软雅黑";

    list-style: none;
    text-decoration: none;
}
a:hover{text-decoration: underline}
body{
    font-size: 14px;
}
.container{
    width: 1289px;
    margin:0 auto;
}
.LF{
    float: left;
}
.RF{
    float: right;
}

/*header样式*/
header{
    overflow: hidden;
}
header #logo{
    padding-right: 234px;
}
header ul{
    padding-top: 23px;
    padding-right: 167px;
}
header ul li{
    float: left;
    padding-right: 15px;
    color: #337AB7;
}

/*nav样式*/
nav{
    background: #222222;
    height: 51px;
    border: 1px solid #000;
    border-radius: 3px;
}
nav ul li{
    float: left;
    line-height: 51px;
}
nav ul li a{
    display: block;
    color: #9D9D9D;
    padding: 0px 15px;
    height: 51px;
}
nav ul li a:hover{
    color: #fff;
    background: #080808;
    text-decoration: none;
}
nav ul li:first-child a{
font-size: 16px;
}
nav div{
    padding-top: 8px;
    padding-right: 30px;
}
nav div #text{
    height: 32px;
    width: 175px;
    border: 1px solid #CCCCCC;
    border-radius: 3px;
    text-indent: 1em;
    color: #9999B2;
}
nav div #submit{
    width: 67px;
    height: 32px;
    background: #ffffff;
    border-radius: 3px;
    border: 1px solid #ccc;
    cursor: pointer;
}

/*show样式*/
.show{
    padding-top: 20px;
}
.show img{
    width: 100%;
}

/*new样式*/
.new .top{
padding-top: 7px;
height: 57px;
line-height: 57px;
padding-left: 17px;
}
.new .top img{
padding-top: 25px;
padding-left: 20px;
}
.new .btm .left img{
    height: 406px;
}
.new .btm ul{overflow: hidden}
.new .btm ul .main{
    text-align: right;
    width: 540px;
    height: 200px;
}
.new .btm ul .main img{
    width: 516px;
    height: 200px;
}
.new .btm ul li{
    width: 180px;
    height: 200px;
    text-align: center;
    float: left;
}
.new .btm ul li .name{
    padding-top: 15px;
}
.new .btm ul li .name a{
    color: #808080;
}
.new .btm ul li .price{
    padding-top: 14px;
    color: #FF0000;
}
.new .btm .down{
    padding-top: 5px;
}

/*pic样式*/
.pic img{
    width: 100%
}

/*footer样式*/
.last{
    text-align: center;
    padding-top: 50px;
    clear: both;
}
.last img{
    width: 100%;
}
.last ul{
    padding-left: 320px;
    overflow: hidden;
}
.last ul li{
    float: left;
    padding-left: 15px;
}
.last ul li a{

    color: #337AB7;
}
.last p{
    padding-top: 10px;
}