/*通用样式*/
*{
    margin: 0;
    padding: 0;
    font-family: "微软雅黑";

    list-style: none;
    text-decoration: none;
}
a:hover{text-decoration: underline}
body{
    font-size: 14px;
}
.container{
    width: 1289px;
    margin:0 auto;
}
.LF{
    float: left;
}
.RF{
    float: right;
}

/*header样式*/
header{
    overflow: hidden;
}
header #logo{
    padding-right: 234px;
}
header ul{
    padding-top: 23px;
    padding-right: 167px;
}
header ul li{
    float: left;
    padding-right: 15px;
    color: #337AB7;
}

/*nav样式*/
nav{
    background: #222222;
    height: 51px;
    border: 1px solid #000;
    border-radius: 3px;
}
nav ul li{
    float: left;
    line-height: 51px;
}
nav ul li a{
    display: block;
    color: #9D9D9D;
    padding: 0px 15px;
    height: 51px;
}
nav ul li a:hover{
    color: #fff;
    background: #080808;
    text-decoration: none;
}
nav ul li:first-child a{
    font-size: 16px;
}
nav div{
    padding-top: 8px;
    padding-right: 30px;
}
nav div #text{
    height: 32px;
    width: 175px;
    border: 1px solid #CCCCCC;
    border-radius: 3px;
    text-indent: 1em;
    color: #9999B2;
}
nav div #submit{
    width: 67px;
    height: 32px;
    background: #ffffff;
    border-radius: 3px;
    border: 1px solid #ccc;
    cursor: pointer;
}

/*form样式*/
.form{
    margin-top: 20px;
    padding-bottom: 20px;
    background: url("../images/regist_bg.jpg") repeat;
    padding-top: 29px;
    border: 1px solid #CCCCCC;

}
.form form{
    padding-top: 30px;
    padding-bottom: 55px;
    padding-left: 90px;
    width: 881px;
    margin: 0 auto;
    border: 7px solid #CCCCCC;
}
.form form p{
    font-size: 16px;
    color: #3164AF;
}
.form form span{
    font-size: 10px;
    color: #333333;
}
.form form table tr{
    display: block;
    padding-top: 15px;
    padding-left: 24px;
}
.form form table tr td{
    font-size: 14px;
    text-align: right;
    width: 60px;
}
.form form table tr td:last-child{
    padding-left: 30px;
}
.form form table tr td:last-child .input{
    width: 344px;
    height: 32px;
    border: 1px solid #CCCCCC;
    border-radius: 4px;
    text-indent: 1em;
}
.form form table tr td:last-child .submit{
    color: #ffffff;
    width: 95px;
    height: 31px;
    background: #CA062C;
    border: 2px solid #DDDDDD;
    margin-left: 60px;
    cursor: pointer;
}



/*footer样式*/
.last{
    text-align: center;
    padding-top: 50px;
    clear: both;
}
.last img{
    width: 100%;
}
.last ul{
    padding-left: 320px;
    overflow: hidden;
}
.last ul li{
    float: left;
    padding-left: 15px;
}
.last ul li a{

    color: #337AB7;
}
.last p{
    padding-top: 10px;
}